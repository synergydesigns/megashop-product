  "license": "ISC",
  "bugs": {
    "url": "https://github.com/synergydesigns/megashop-product/issues"
  },
  "homepage": "https://github.com/synergydesigns/megashop-product#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-validation": "^1.0.2",
    "joi": "^13.0.1",
    "js-logger": "^1.4.1",
    "morgan": "^1.9.0",
    "pg": "^7.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.20.1",
    "sequelize-cli": "^3.0.0"
