    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-plugin-import": "^2.2.0",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "supertest": "^3.0.0"
